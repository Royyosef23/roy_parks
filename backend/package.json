{
  "name": "parkbnb-backend",
  "version": "1.0.0",
  "description": "Backend API for ParkBnB - parking rental platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset --force",
    "docker:build": "docker build -t parkbnb-backend .",
    "docker:run": "docker run -p 3001:3001 parkbnb-backend"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@roy-parks/shared": "file:../shared",
    "@types/bcrypt": "^6.0.0",
    "@types/nodemailer": "^6.4.17",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.4",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.5",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "author": "Roy Yosef",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "keywords": []
}
